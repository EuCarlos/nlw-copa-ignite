// npx prisma init --datasource-provider SQLite

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pool {
  id          String @id @default(cuid())
  title       String
  code        String @unique
  created_at  DateTime @default(now())
}